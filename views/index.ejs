<div class="w-[90vw] mx-auto">
    <div class="flex justify-center h-24">
        <h1 class="text-3xl"><%= title %></h1>
    </div>
    
    <div><p>hai, <%= name %></p></div>
    <% if (!galeries.length) { %>
        <div>galeri kosong</div>
        <% }  %>
        <% galeries.forEach(galeri => {   %>
        <div class="flex flex-col gap-2 mb-4">
            <div><h2 class="text-2xl"><%= galeri.judul %></h2></div>
            <div class="w-52"><img src="/img/galeri/<%= galeri.gambar %>" alt="<%= galeri.gambar %>" class="aspect-square object-cover"></div>
        </div>
        <% })  %>
</div>